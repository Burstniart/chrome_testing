{
    "name": "Page Greenner",
    "version": "2.2",
    "description": "Clicking will change page green, Alt+Shift+P will pin current tab",
    "permissions": [
      "activeTab",
      "scripting",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "manifest_version": 3,
    "commands": {
      "toggle-pin": {
        "suggested_key": { "default": "Alt+Shift+P" },
        "description": "Tooggle tab pin"
      },
      "alert_shortcut": {
        "suggested_key": { "default": "Alt+Shift+0" },
        "description": "Alert shortcut was executed"
      }
    },
     "action": {
      "default_title": "Test icon and badge",
      "default_popup": "popup.html"
    }, 
    "icons": {
    "16": "drink_water16.png",
    "32": "drink_water32.png",
    "48": "drink_water48.png",
    "128": "drink_water128.png"
    }
  }